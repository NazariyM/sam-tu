									ul.header__nav-menu
										p Исполнительное производство
										li: a(href='#!') Исполнительное производство
										li: a(href='#!') Составление претензий, исковых заявлений

							li
								+e.A.nav-icon._05(href='#!')
									span Перевод жилых помещений

							li
								+e.A.nav-icon._06(href='#!')
									span Абонентсоке юридическое обслуживание

							li
								+e.A.nav-icon._07(href='#!')

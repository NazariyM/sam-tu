										li: a(href='#!') Перевод жилых помещений

							li
								+e.A.nav-icon._04
									span Исполнительное производство

									ul.header__nav-menu
										p Исполнительное производство
										li: a(href='#!') Исполнительное производство
										li: a(href='#!') Составление претензий, исковых заявлений

							li
								+e.A.nav-icon._07
									span Банкротство физ. лиц
									ul.header__nav-menu
										p Банкротство физ. лиц
										li: a(href='#!') Антиколлектор
										li: a(href='#!') Банкротство физлиц
										li: a(href='#!') Наследство

							li
								+e.A.nav-icon._09(href='#!')
									span Защита прав потребителей

							li
								+e.A.nav-icon._08(href='#!')
									span Помощь при банкротстве страховых

					li: a(href='#!')
						span цены на услуги
					li: a(href='#!')
						span бухгалтерские услуги

			+e.BUTTON.mob-call-btn(type='button') ЗАКАЗАТЬ ЗВОНОК

			+e.search-field.search-field

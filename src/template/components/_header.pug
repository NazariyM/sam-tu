//begin header
+b.HEADER.header(class= headerClass ? headerClass : '')
	+e.top
		+e.top-container.container
			-
				menuList = [
				'О компании',
				'Акции',
				'Отзывы',
				'Видео',
				'Новости',
				'Полезное',
				'Контакты',
				'Соискателям',
				'Вопрос/Ответ'
				]

			ul
				for menuText in menuList
					li: a(href='#!')= menuText

			ul
				li: a(href='#!') Бухгалтерия
				li: a(href='#!') Оценка
				li: a(href='#!') Экспертиза
				li: a(href='#!') ООО
				li: a(href='#!') ИП

			+e.top-tels
				a(href='tel:+78462767888')
					span +7 (846)
					= ' '
					| 27-67-888
				a(href='tel:+78462766888')
					span +7 (846)
					= ' '
					| 27-66-888

	+e.mid
		+e.container.container
			+e.row
				+e.mid-col
					+e.A.logo.logo(href='#!')
						+e.IMG(src='static/img/other/logo.png' alt='Технология управления')
				+e.mid-col.address-block
					button.address-block__btn.btn.js-popup-btn(type='button')
						span.address-block__mob-text Ваш город:
						span Самара
						svg(class='address-block__btn-icon' xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', width='12', height='14', viewbox='0 0 12 14')
							defs
								path#7zrna(d='M634 118.2l-1.2-1.24-2.92 2.88V113l-1.76.02v6.8l-2.94-2.84-1.18 1.2 5 4.82z')
							g
								g(transform='translate(-623 -111)')
									use(fill='#fff', xlink:href='#7zrna')

					.address-block__text
						p Московское шоссе д. 3 офисы: 301Б, 302, 309
						p
							strong Время работы:
							= ' '
							| Пн-пт, 09:00-18:00

				+e.mid-col.tel-block
					p Наши телефоны:
					a(href='tel:+78462767888')
						span +7 (846)
						= ' '
						| 27-67-888
					a(href='tel:+78462766888')
						span +7 (846)
						= ' '
						| 27-66-888

				+e.mid-col
					+e.A.btn.btn._red-line(href='#!') Заказать звонок
					+e.A.btn.btn._grey-line(href='#!') Задать вопрос

	+e.bot
		+e.bot-container.container
			+b.BUTTON.nav-btn(type='button')
				+e.SPAN.icon
				+e.SPAN.text Меню

			+e.NAV.nav
				ul
					li
						a(href='#!').nav-dropdown-btn
							span услуги для юридических лиц
						ul
							li
								+e.A.nav-icon._01
									span Споры

								ul.header__nav-menu
									p Споры
									li: a(href='#!') Представительство в суде
									li: a(href='#!') Перевод жилых помещений
									li: a(href='#!') Трудовые
									li: a(href='#!') Налоговые
									li: a(href='#!') Земельные споры
									li: a(href='#!') Арбитражные споры. Арбитраж
									li: a(href='#!') Жилищные споры
									li: a(href='#!') Административные споры
									li: a(href='#!') Приватизация
									li: a(href='#!') Споры с ГИБДД
									li: a(href='#!') Оформление перепланировок
									li: a(href='#!') Споры с пожарными (ГПН при МЧС)

							li
								+e.A.nav-icon._02
									span Регистрация

								ul.header__nav-menu
									p Регистрация
									li: a(href='#!') Предоставление юридических адресов
									li: a(href='#!') Предприятий
									li: a(href='#!') Обособленные подразделения, филиалы, представительства
									li: a(href='#!') НКО
									li: a(href='#!') Изменения в ЕГРЮЛ, ЕГРИП
									li: a(href='#!') Регистрация ПАО
									li: a(href='#!') ООО
									li: a(href='#!') ИП

							li
								+e.A.nav-icon._03
									span Кадастровая оценка

									ul.header__nav-menu
										p Кадастровая оценка
										li: a(href='#!') Снижение налога на недвижимость
										li: a(href='#!') Кадастровая оценка. Оспаривание кадастровой стоимости
										li: a(href='#!') Кадастровая оценка земельных участков
										li: a(href='#!') Кадастровая оценка нежилых помещений
										li: a(href='#!') Кадастровая оценка квартир
										li: a(href='#!') Кадастровая оценка частных домов и коттеджей

							li
								+e.A.nav-icon._04
